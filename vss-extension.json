{
    "manifestVersion": 1,
    "id": "lambda3docker",
    "version": "0.2.3",
    "name": "Docker build task",
    "publisher": "lambda3",
    "description": "Adds a build task that enables Docker actions.",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "lambda3docker/icon.svg"
    },
    "categories": [
        "Build and release"
    ],
    "tags": [
        "docker",
        "container",
        "release",
        "docker-compose",
        "container services",
        "build"
    ],
    "screenshots": [
        {
            "path": "screenshots/build.png"
        },
        {
            "path": "screenshots/release.png"
        }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE.txt"
        }
    },
    "links": {
        "home": {
            "uri": "https://www.lambda3.com.br"
        },
        "repository": {
            "uri": "https://github.com/Lambda3/vsts-build-task-docker"
        },
        "issues": {
            "uri": "https://github.com/Lambda3/vsts-build-task-docker/issues"
        }
    },
    "branding": {
        "color": "rgb(13, 92, 155)",
        "theme": "dark"
    },
    "contributions": [
        {
            "id": "showCommits",
            "type": "ms.vss-distributed-task.task",
            "description": "Adds a build task that enables Docker actions.",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "lambda3docker"
            }
        }
    ],
    "files": [
        {
            "path": "lambda3docker"
        }
    ],
    "galleryFlags": [
      "Public",
      "Preview"
    ]
}
